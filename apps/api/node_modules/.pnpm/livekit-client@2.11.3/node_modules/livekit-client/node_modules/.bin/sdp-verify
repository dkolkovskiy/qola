#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/denis.kolkovskiy/QOLA/apps/api/node_modules/.pnpm/sdp-transform@2.15.0/node_modules/sdp-transform/node_modules:/Users/denis.kolkovskiy/QOLA/apps/api/node_modules/.pnpm/sdp-transform@2.15.0/node_modules:/Users/denis.kolkovskiy/QOLA/apps/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/denis.kolkovskiy/QOLA/apps/api/node_modules/.pnpm/sdp-transform@2.15.0/node_modules/sdp-transform/node_modules:/Users/denis.kolkovskiy/QOLA/apps/api/node_modules/.pnpm/sdp-transform@2.15.0/node_modules:/Users/denis.kolkovskiy/QOLA/apps/api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sdp-transform@2.15.0/node_modules/sdp-transform/checker.js" "$@"
else
  exec node  "$basedir/../../../../../sdp-transform@2.15.0/node_modules/sdp-transform/checker.js" "$@"
fi
